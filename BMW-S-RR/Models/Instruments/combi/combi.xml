	<animation>
	  <type>select</type>
	  <object-name>lights</object-name>
	  <condition>
	 	<property>/sim/rendering/rembrandt/enabled</property>
		<greater-than>
	 		<property>/sim/time/sun-angle-rad</property>
			<value>1.4</value>
		</greater-than>
	 </condition>
	</animation>
	
    <animation>
      <type>select</type>
		 <object-name>CTRL.eng</object-name>
      <condition>
          <property>controls/Motorcycle/ctrl-light-overspeed</property>
      </condition>
    </animation>
	
    <animation>
      <type>select</type>
		 <object-name>warning</object-name>
		 <object-name>gearbox</object-name>
      <condition>
        <not>
          <property>engines/engine/running</property>
        </not>
      </condition>
    </animation>
	
    <animation>
      <type>select</type>
		 <object-name>fuel</object-name>
      <condition>
		  <or>
	        <less-than>
	          <property>consumables/fuel/total-fuel-lbs</property>
			  <value>5.0</value>
	        </less-than>
	        <not>
	          <property>engines/engine/running</property>
	        </not>
		  </or>
      </condition>
    </animation>
 
    <animation>
      <type>select</type>
      <object-name>neutral</object-name>
      <condition>
        <equals>
          <property>engines/engine/gear</property>
          <value>0</value>
        </equals>
      </condition>
    </animation>

    <animation>
      <type>select</type>
		 <object-name>speed-li-warn</object-name>
      <condition>
          <property>engines/engine/rpm_regulation</property>
      </condition>
    </animation>

	<animation>
	  <type>rotate</type>
	  <object-name>RPM.btn</object-name>
	  <property>engines/engine/rpm_regulation</property>
      <factor>-67</factor>
      <center>
