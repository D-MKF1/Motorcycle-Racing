    <animation>
      <type>select</type>		 
	  	 <object-name>scs</object-name>
		 <object-name>oil.btn</object-name>
      <condition>
		  <or>
	        <not>
	          <property>engines/engine/running</property>
	        </not>
	  	  	<property>controls/Honda-RC213V/SCS/ctrl-light</property>
		</or>
      </condition>
    </animation>

    <animation>
      <type>select</type>		 
	  	 <object-name>abs</object-name>
      <condition>
		  <or>
	        <not>
	          <property>engines/engine/running</property>
	        </not>
	        <property>controls/Honda-RC213V/ABS/ctrl-light</property>
		</or>
      </condition>
    </animation>
	
    <animation>
      <type>select</type>		 
	  	 <object-name>overspeed</object-name>
      <condition>
		  <or>
	        <not>
	          <property>engines/engine/running</property>
	        </not>
	        <property>controls/Honda-RC213V/ctrl-light-overspeed</property>
		</or>
      </condition>
    </animation>

    <animation>
      <type>select</type>
		 <object-name>fuel</object-name>
      <condition>
		  <or>
	        <less-than>
	          <property>consumables/fuel/total-fuel-lbs</property>
			  <value>5.0</value>
	        </less-than>
	        <not>
	          <property>engines/engine/running</property>
	        </not>
		  </or>
      </condition>
    </animation>

    <animation>
      <type>select</type>
      <object-name>neutral</object-name>
      <condition>
        <less-than>
          <property>engines/engine/gear</property>
          <value>1</value>
        </less-than>
      </condition>
    </animation>
	
	<model>
	<path>Aircraft/Honda-RC213V/Models/Lights/lightcone-mini.xml</path>
	<name>lights</name>
	<offsets>   
	  <x-m> 0.25</x-m>
	  <y-m> 0.03</y-m>
	  <z-m> 0.08</z-m>
	  <pitch-deg>75</pitch-deg>
	</offsets>
	</model>
	
	<animation>
	  <type>select</type>
	  <object-name>lights</object-name>
	  <condition>
	 	<property>/sim/rendering/rembrandt/enabled</property>
		<greater-than>
	 		<property>/sim/time/sun-angle-rad</property>
			<value>1.4</value>
		</greater-than>
	 </condition>
	</animation>
	
