    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>Panigale.png</texture>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>staender</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/float[3]</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
	  <type>rotate</type>
      <object-name>staender</object-name>
      <property>sim/multiplay/generic/float[3]</property>
      <factor>48</factor>
      <center>
       <x-m> 0.771</x-m>
       <y-m> 0.0</y-m>
       <z-m> 0.196</z-m>
      </center>
      <axis>
       <x>0</x>
       <y>1</y>
       <z>0</z>
      </axis>
    </animation>

<effect>
	<inherits-from>Effects/model-transparent</inherits-from>
	<object-name>scheibe</object-name>
</effect>
  
  <!-- shadow without rembrandt -->
	<model>
		<name>NewShadow</name>
		<path>Aircraft/Motorcycle-Racing/Ducati/Models/shadow.xml</path>
	    <offsets>
	      <x-m>  0.0 </x-m>
	      <y-m>  0.0 </y-m>
	      <z-m>  0.03 </z-m>
	    </offsets>
	</model>

	<animation>
		<type>select</type>
		<object-name>NewShadow</object-name>
		<condition>
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
			<not><property>sim/multiplay/generic/int[3]</property></not>
		</condition>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>shadow</object-name>
		<property>surface-positions/left-aileron-pos-norm</property>
		<factor>0.5</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
	  <type>scale</type>
	  <object-name>shadow</object-name>
	  <property>surface-positions/left-aileron-pos-norm</property>
	  <x-min>1.0</x-min>
	  <y-min>1.0</y-min>
	  <z-min>1.0</z-min>
	  <x-factor>1.0</x-factor>
	  <y-factor>1.8</y-factor>
	  <z-factor>0.0</z-factor>
	 </animation>
	 
 	<animation>
 	  <type>rotate</type>
 	  <object-name>shadow</object-name>
 	  <property>orientation/pitch-deg</property>		
	  <factor>-1</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
 	 </animation>

  <!-- 1. SPEED --> 
  <model>
	<name>RPM</name>
    <path>Aircraft/Motorcycle-Racing/Ducati/Models/Instruments/combi/combi.xml</path>
    <offsets>
      <x-m> -0.76 </x-m>
      <y-m>-0.075 </y-m>
      <z-m> 0.93 </z-m>
      <pitch-deg> -20 </pitch-deg>
    </offsets>
  </model>
  
  <model>
	<name>TRANSPONDER</name>
    <path>Aircraft/Motorcycle-Racing/Ducati/Models/Instruments/transponder/transponder.xml</path>
    <offsets>
        <x-m>-0.757</x-m>
        <y-m>-0.055</y-m>
        <z-m> 0.86 </z-m>
      <heading-deg>90</heading-deg>
      <roll-deg> 46 </roll-deg>
    </offsets>
  </model>
  
  <animation>
    <type>select</type>
    <object-name>TRANSPONDER</object-name>
    <condition>
	 <or>
		<equals>
        	<property>sim/menubar/default/menu[14]/item[0]/name</property>
			<value>Button_1</value>
		</equals>
		<equals>
        	<property>i-am-a-racebike</property>
			<value>1</value>
		</equals>		
	 </or>
    </condition>
  </animation>
  
  <model>
	<name>SMOKE</name>
    <path>Aircraft/Motorcycle-Racing/MR-Effects/smoke.xml</path>
    <offsets>
      <x-m> -0.352 </x-m>
      <y-m>  0.0 </y-m>
      <z-m>  0.43 </z-m>
    </offsets>
  </model>
  
  <model>
	<name>SMOKE-LIGHT</name>
    <path>Aircraft/Motorcycle-Racing/MR-Effects/smoke-light.xml</path>
    <offsets>
      <x-m> -0.352 </x-m>
      <y-m>  0.0 </y-m>
      <z-m>  0.43 </z-m>
    </offsets>
  </model>
   
  <animation>
    <type>select</type>
    <object-name>SMOKE</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/float[4]</property>
		<value>0.9</value>
      </greater-than>
    </condition>
  </animation>
   
  <animation>
    <type>select</type>
    <object-name>SMOKE-LIGHT</object-name>
    <condition>
      <greater-than>
        <property>sim/multiplay/generic/float[4]</property>
		<value>0.2</value>
      </greater-than>
    </condition>
  </animation>

  	<animation>
  		 <type>material</type>
  		 <object-name>rearlight</object-name>
  		 <emission>
  		  <red>1</red>
  		  <green>1</green>
  		  <blue>1</blue>
  		  <factor>2</factor>
  	   	</emission>
  	</animation>
  
<animation>
  <type>select</type>
  <object-name>rearlight</object-name>
  <condition>
	  <and>
		<property>engines/engine/rpm</property>
		<greater-than>
			<property>/sim/time/sun-angle-rad</property>
			<value>1.4</value>
		</greater-than>
	</and>
 </condition>
</animation>

