<PropertyList>
  <path>Panigale_S.ac</path> 
  
  <sound><path>Aircraft/Motorcycle-Racing/Ducati/Sounds/mp-sound.xml</path></sound>

  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/Motorcycle-Racing/Ducati/Models/Liveries-S", 10);
    </load>

    <unload>
      livery_update.stop();
    </unload>
  </nasal>
  
  <offsets>
    <z-m>0.12</z-m>
  </offsets>

  <animation>
    <type>material</type>
    <object-name>auspuff</object-name>
