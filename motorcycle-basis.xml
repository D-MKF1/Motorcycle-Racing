<PropertyList>
 <sim>
		<description>Motorcycle-Racing</description>
		<author>LAKE OF CONSTANCE HANGAR :: Marc Kraus</author>
		<aircraft-version>0.0.1</aircraft-version>

		<fuel-fraction>1.0</fuel-fraction>

		<systems>
			<electrical>
			<path>Aircraft/Generic/generic-electrical.xml</path>
			</electrical>
		</systems> 

		<hud>
			<enable3d>false</enable3d>
		</hud>

  <help>
		    <key>
		      <name>Home or ></name>
		      <desc>Shift up</desc>
		    </key>
		    <key>
		      <name>End or Shift + ></name>
		      <desc>Shift down</desc>
		    </key>
		    <key>
		      <name>Backspace or Ctrl key</name>
		      <desc>Clutch</desc>
		    </key>
			<key>
			  <name>s</name>
			  <desc>engine starter</desc>
			</key>
			<key>
			  <name>g/G and b/B</name>
			  <desc>toggle workstand</desc>
			</key>
			<key>
			  <name>1</name>
			  <desc>Quick glance over the shoulder</desc>
			</key>
			<key>
			  <name>F7</name>
			  <desc>Open map</desc>
			</key>
			<key>
			  <name>F8</name>
			  <desc>Open results</desc>
			</key>
			<key>
			  <name>l</name>
			  <desc>switch last and fastest lap</desc>
			</key>
			<key>
			  <name>F9</name>
			  <desc>Open fuel menu</desc>
			</key>
		<text>*** ISLE OF MAN EDITION ***
"Ladies and Gentleman, start your engines"
and have fun!</text>
  </help>
	
	<multiplay>
		 <generic>
  			<float n="0" alias="/gear/gear[0]/rollspeed-ms"/> <!-- wheel rotation -->
  			<float n="1" alias="/gear/gear[1]/rollspeed-ms"/> <!-- wheel rotation -->
  			<float n="2" alias="/gear/gear[0]/compression-norm"/> <!-- front wheel supension -->
  			<float n="3" alias="/controls/Motorcycle/driver-up"/> <!-- animation for pilot in workstand -->
  			<float n="4" alias="/engines/engine/killed"/> <!-- animation broken engine in workstand -->
  			<float n="5" alias="/gear/gear[1]/compression-norm"/>
  			<float n="6" alias="/Motorcycle/this-lap-time"/> <!-- for mp -->
  			<float n="7" alias="/Motorcycle/this-race-time"/> <!-- for mp -->
  			<float n="8" alias="/Motorcycle/last-lap-time"/> <!-- for mp -->
            <float n="9" alias="/position/altitude-agl-ft"/>

  			<int n="0" alias="/controls/Motorcycle/newbie"/> <!-- animation red security pullover -->
  			<int n="1" alias="/Motorcycle/race-lap"/> 	 <!-- mp result views -->
  			<int n="2" alias="/controls/flag-info"/>  <!-- for the livetiming roadracer script -->
  			<int n="3" alias="/sim/crashed"/>  <!-- no driver if crashed -->
  			<int n="4" alias="/controls/Motorcycle/driver-looks-back"/>  <!-- driver looks back over his shoulder -->
