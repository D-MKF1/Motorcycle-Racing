<input>
	<keyboard>		
		<key n="114"> 
         <name>recording</name>
         <desc>lap record</desc>         
		 <binding>
		  <command>property-toggle</command>		  
		  <property>/virtual-racer/recording</property>
	     </binding>
		</key>
		<key n="82"> 
         <name>playing</name>
         <desc>virtual racer on track</desc>         
		 <binding>
		  <command>property-toggle</command>		  
		  <property>/virtual-racer/play</property>
	     </binding>
		</key>
		<key n="108"> 
         <name>Last or fastest</name>
         <desc>last or fastest lap times</desc>
         <binding>
		  <command>property-toggle</command>
		  <property>/Motorcycle/switch-last-fastest</property>
	     </binding>
		</key>
	  <!--shift up/down-->
        <key n="362"> 
         <name>Home</name>
         <desc>Shift Up</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
        <key n="363"> 
          <name>End</name>
          <desc>Shift Down</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">-1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
        <key n="60"> 
         <name>></name>
         <desc>Shift Up Alternative</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
        <key n="62"> 
          <name>Shift + ></name>
          <desc>Shift Down Alternative</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">-1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
		<key n="8"> 
         <name>Backspace</name>
         <desc>Clutch</desc>
         <binding>
		  <command>property-assign</command>
		  <property>/engines/engine/clutch</property>
		  <value>1</value>
	     </binding>
		 <mod-up>
		  <binding>
           <command>property-assign</command>
		   <property>/engines/engine/clutch</property>
		   <value>0</value>
          </binding>
		 </mod-up>
		</key>
		<key n="83"> 
         <name>Start/Stop</name>
         <desc>Clutch</desc>
         <binding>
		  <command>property-toggle</command>
		  <property>sim/model/start-idling</property>
		  <value>1</value>
	     </binding>
		</key>
		<key n="115"> 
         <name>Start/Stop</name>
         <desc>Clutch</desc>
         <binding>
		  <command>property-toggle</command>
		  <property>sim/model/start-idling</property>
		  <value>1</value>
	     </binding>
		</key>
		
		<!-- Clutch with Ctrl find setlistener in gearbox.nas -->	
		<key n="263">
			<name>F7</name>
			<desc>Show map</desc>
			<binding>
			<command>dialog-show</command>
			<dialog-name>map</dialog-name>
			</binding>
		</key>
		<key n="264">
		    <name>F8</name>
		    <desc>Results</desc>
			<binding>
				<command>nasal</command>
				<script>
					var s1 = getprop("/Motorcycle/show-race-times") or 0;
					var s2 = getprop("/road-racer/show-mp-times") or 0;
					if(s1 or s2){
						setprop("/Motorcycle/show-race-times", 0);
						setprop("/road-racer/show-mp-times", 0);
					}else{
						setprop("/Motorcycle/show-race-times", 1);
						setprop("/road-racer/show-mp-times", 1);
					}
				</script>
			</binding>
		</key>
		<key n="265">
		    <name>F9</name>
		    <desc>Pop up Fuel and Weight dialog</desc>
		  	<binding>
		  		<command>nasal</command>
		  		<script>gui.showWeightDialog()</script>
		  	</binding>
		</key>

	</keyboard>
