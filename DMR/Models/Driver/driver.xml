<PropertyList>

  <path>driver.ac</path>	
  <animation>
    <type>noshadow</type>
    <object-name>helm.001</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>helm.001</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>corps</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
  	<object-name>hair</object-name>
    <object-name>helm</object-name>
    <object-name>tete</object-name>
    <condition>
		<or>
	      <equals>
	        <property>sim/model/livery/file</property>
	        <value>white</value>
	      </equals>
	      <equals>
	        <property>sim/model/livery/file</property>
	        <value>black</value>
	      </equals>
	      <equals>
	        <property>sim/model/livery/file</property>
	        <value>red</value>
	      </equals>
	      <equals>
	        <property>sim/model/livery/file</property>
	        <value>yellow</value>
	      </equals>
	  	</or>
    </condition>
    <texture>driver.png</texture>
  </animation>

  <animation>
    <type>material</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>corps</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
  	<object-name>hair</object-name>
    <object-name>helm</object-name>
    <object-name>tete</object-name>
    <condition>
      <equals>
        <property>sim/model/livery/file</property>
        <value>harrison</value>
      </equals>
    </condition>
    <texture>Harrison_driver.png</texture>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>corps</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
  	<object-name>hair</object-name>
    <object-name>helm</object-name>
    <object-name>tete</object-name>
    <condition>
      <equals>
        <property>sim/model/livery/file</property>
        <value>holden</value>
      </equals>
    </condition>
    <texture>JohnHolden_driver.png</texture>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>corps</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
  	<object-name>hair</object-name>
    <object-name>helm</object-name>
    <object-name>tete</object-name>
    <condition>
      <equals>
        <property>sim/model/livery/file</property>
        <value>reeves</value>
      </equals>
    </condition>
    <texture>Green_driver.png</texture>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>corps</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
  	<object-name>hair</object-name>
    <object-name>helm</object-name>
    <object-name>tete</object-name>
    <condition>
      <equals>
        <property>sim/model/livery/file</property>
        <value>tinmouth</value>
      </equals>
    </condition>
    <texture>JennyTinmouth_driver.png</texture>
  </animation>
  
  <animation>
  <type>select</type>
  <object-name>hair</object-name>
	  <condition>
	    <equals>
	      <property>sim/model/livery/file</property>
	      <value>tinmouth</value>
	    </equals>
	  </condition>
  </animation>

  <animation>
  <type>select</type>
  <object-name>tete</object-name>
  <object-name>helm</object-name>
  <object-name>helm.001</object-name>
  <object-name>corps</object-name>
  <object-name>gilet</object-name>
  <object-name>hair</object-name>
    <condition>
      <not>
        <equals>
        <property>sim/current-view/name</property>
        <value type="string">Cockpit View</value>
        </equals>
      </not>
    </condition>
  </animation>
  
  <animation>
  <type>select</type>
  <object-name>gilet</object-name>
    <condition>
      	  <property>sim/multiplay/generic/int[0]</property>
    </condition>
  </animation>
  
  <!-- animation while driving -->
  
  <animation>
    <type>rotate</type>
    <object-name>tete</object-name>
    <object-name>helm</object-name>
    <object-name>helm.001</object-name>
    <object-name>hair</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>-10</factor>
    <center>
      <x-m>-0.134 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 1.21 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation> 
     
  <animation>
    <type>rotate</type>
    <object-name>brasD</object-name>
    <object-name>avantbrasD</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>-4</factor>
    <center>
      <x-m>-0.3795 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.6219 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation> 
     
  <animation>
    <type>rotate</type>
    <object-name>brasG</object-name>
    <object-name>avantbrasG</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>4</factor>
    <center>
      <x-m>-0.3795 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.6219 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation> 

  <animation>
      <type>translate</type>
      <object-name>avantbrasD</object-name>
      <property>surface-positions/rudder-pos-norm</property>
      <factor>0.036</factor>
      <axis>
        <x> 1 </x>
        <y> 0 </y>
        <z> 0 </z>
      </axis>
  </animation>  
  
  <animation>
      <type>translate</type>
      <object-name>avantbrasG</object-name>
      <property>surface-positions/rudder-pos-norm</property>
      <factor>-0.044</factor>
      <axis>
        <x> 1 </x>
        <y> 0 </y>
        <z> 0 </z>
      </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>tete</object-name>
    <object-name>helm</object-name>
    <object-name>helm.001</object-name>
 	<object-name>hair</object-name>
    <condition>
      <greater-than>
        <property>velocities/groundspeed-kt</property>
        <value>5</value>
      </greater-than>
    </condition>
    <property>surface-positions/left-aileron-pos-norm</property>
    <factor>50</factor>
    <center>
      <x-m> 0.0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.5797 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!-- animation in stand on wheelsholder -->
  <animation>
    <type>rotate</type>
 	<object-name>hair</object-name>
    <property>sim/multiplay/generic/float[3]</property>
    <factor>5</factor>
    <center>
      <x-m>-0.427 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.7487 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>tete</object-name>
    <object-name>helm</object-name>
    <object-name>helm.001</object-name>
 	<object-name>hair</object-name>
    <object-name>corps</object-name>
    <object-name>gilet</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/left-aileron-pos-norm</property>
        <value>0</value>
      </greater-than>
    </condition>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
	    <entry><ind>  0</ind><dep>  0</dep></entry>
	    <entry><ind>0.15</ind><dep>-9</dep></entry>
	    <entry><ind>1.00</ind><dep>-9</dep></entry>
    </interpolation>
    <center>
      <x-m> 0.182 </x-m>
      <y-m> 0.011 </y-m>
      <z-m> 0.622 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
  
